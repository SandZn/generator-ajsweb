{
    "name": "generator-angularng",
    "version": "1.0.0",
    "description": "Yeoman Gulp generator for AngularJS ",
    "homepage": "https://github.com/yadickson/generator-angularng",
    "author": {
        "name": "Yadickson Soto",
        "email": "yadickson@gmail.com",
        "url": "https://github.com/yadickson"
    },
    "files": [
        "app"
    ],
    "main": "dist/generator-angularng.js",
    "keywords": [
        "yeoman gulp angularjs mocha chai karma",
        "yeoman-generator"
    ],
    "dependencies": {
        "chalk": "^1.1.3",
        "yeoman-generator": "^2.0.1",
        "yosay": "^2.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-babili": "^0.1.4",
        "babel-preset-env": "^1.6.1",
        "coveralls": "^2.13.1",
        "mkdirp": "^0.5.1",
        "nyc": "^11.2.1",
        "rimraf": "^2.6.2",
        "yeoman-assert": "^3.0.0",
        "yeoman-test": "^1.6.0"
    },
    "scripts": {
        "clean": "./node_modules/.bin/rimraf coverage reports *.tgz",
        "prepublish": "npm run compile",
        "precompile": "./node_modules/.bin/mkdirp dist",
        "compile": "./node_modules/.bin/babel --presets env,babili --out-file $npm_package_main app",
        "test": "./node_modules/.bin/nyc --reporter=html mocha --recursive",
        "coveralls": "./node_modules/.bin/nyc report --reporter=text-lcov | coveralls"
    },
    "repository": "https://github.com/yadickson/generator-angularng.git",
    "license": "GPL-3.0"
}
